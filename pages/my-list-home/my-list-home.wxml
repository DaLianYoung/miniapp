<view class="page">
  <view wx:if="{{role == 21}}" style="text-align: center; font-size: 18px; line-height: 100px;">
    您好：服务者，请您联系管理维护您的角色
  </view>
  <view wx:else>
    <view class="my-list-item" bindtap="goToWsList" wx:if="{{role == 1 || role == 2 || role == 3 || role == 4 || role == 5 || role == 6 || role == 7 || role == 10 || role == 11 || role == 12 || role == 13 || role == 14 || role == 18 || role == 22}}">
      <view class="fr">
        <van-icon name="arrow" style="font-size: 18px; vertical-align: middle; line-height: 40px;" />
      </view>
      <view><van-icon name="exchange" style="font-size: 24px; vertical-align: middle; margin-right: 15px; color:#016eff;" />物损工单</view>
    </view>
    <view class="my-list-item" bindtap="goToJcList" wx:if="{{role == 1 || role == 2 || role == 3 || role == 4 || role == 5 || role == 6 || role == 7 || role == 8 || role == 10 || role == 11 || role == 12 || role == 13 || role == 14 || role == 15 || role == 22}}">
      <view class="fr">
        <van-icon name="arrow" style="font-size: 18px; vertical-align: middle; line-height: 40px;" />
      </view>
      <view><van-icon name="shop" style="font-size: 24px; vertical-align: middle; margin-right: 15px; color:#016eff;" />家财工单</view>
    </view>
    <view class="my-list-item" bindtap="goToCxList" wx:if="{{role == 1 || role == 2 || role == 3 || role == 4 || role == 5 || role == 6 || role == 7 || role == 13 || role == 14 || role == 16 || role == 17 || role == 22}}">
      <view class="fr">
        <van-icon name="arrow" style="font-size: 18px; vertical-align: middle; line-height: 40px;" />
      </view>
      <view><van-icon name="logistics" style="font-size: 24px; vertical-align: middle; margin-right: 15px; color:#016eff;" />车险工单</view>
    </view>
    <view class="my-list-item" bindtap="goToLockList" wx:if="{{role == 1 || role == 2 || role == 3 || role == 4 || role == 5 || role == 6 || role == 7 || role == 19 || role == 22}}">
      <view class="fr">
        <van-icon name="arrow" style="font-size: 18px; vertical-align: middle; line-height: 40px;" />
      </view>
      <view><van-icon name="certificate" style="font-size: 24px; vertical-align: middle; margin-right: 15px; color:#016eff;" />开锁工单</view>
    </view>
    <view class="my-list-item" bindtap="goToPipeList" wx:if="{{role == 1 || role == 2 || role == 3 || role == 4 || role == 5 || role == 6 || role == 7 || role == 20 || role == 22}}">
      <view class="fr">
        <van-icon name="arrow" style="font-size: 18px; vertical-align: middle; line-height: 40px;" />
      </view>
      <view><van-icon name="exchange-record" style="font-size: 24px; vertical-align: middle; margin-right: 15px; color:#016eff;" />疏通管道工单</view>
    </view>
  </view>
</view>
